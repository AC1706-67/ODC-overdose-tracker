{
  "expo": {
    "name": "ODC Overdose Tracker",
    "slug": "odc-overdose-tracker",
    "owner": "dr.ecovery",
    "scheme": "odc",
    "version": "1.1.0",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.odc.overdose",
      "buildNumber": "1",
      "infoPlist": {
        "NSHealthShareUsageDescription": "This app helps track overdose incidents for public health purposes.",
        "NSLocationWhenInUseUsageDescription": "Location is used to improve incident reporting accuracy.",
        "CFBundleDisplayName": "ODC Overdose Tracker"
      }
    },
    "android": {
      "package": "org.odc.overdose",
      "versionCode": 3
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9add2f5e-6eee-4906-b050-441c9e963140"
      }
    }
  }
}